{
  "name": "@romcal/data",
  "version": "0.0.0",
  "private": true,
  "description": "Romcal data",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@7.27.0",
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prebuild": "npm run lint",
    "prettier": "prettier -c --config .prettierrc {.github,./*}",
    "prettier:fix": "prettier -w --config .prettierrc {.github,./*}",
    "build": "tsup ./src/index.ts --format cjs,esm,iife --dts --minify --sourcemap --external i18next",
    "test": "jest --coverage",
    "test:without-coverage": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@romcal/core": "workspace:*",
    "i18next": "^21.10.0"
  },
  "devDependencies": {
    "@romcal/jest-config": "workspace:*",
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "jest-extended": "^3.1.0",
    "romcal": "workspace:*",
    "tsup": "^6.6.3"
  }
}
