    }
  ],
  "bugs": {
    "url": "https://github.com/romcal/romcal/issues"
  },
  "homepage": "https://github.com/romcal/romcal/blob/dev/packages/core/README.md",
  "dependencies": {
    "@romcal/data": "workspace:*",
    "i18next": "^21.10.0"
  },
  "devDependencies": {
    "tsup": "^6.6.3"
  },
  "license": "MIT"
}
