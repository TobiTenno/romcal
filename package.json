{
  "name": "romcal",
  "version": "3.0.0-dev.31",
  "description": "JavaScript library that generates liturgical calendars of the Roman Rite of the Roman Catholic Church.",
  "main": "dist/cjs/romcal.js",
  "module": "dist/esm/romcal.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=18.11.0"
  },
  "packageManager": "pnpm@7.28.0",
  "scripts": {
    "clean": "rimraf coverage dist tmp packages/scripts/tmp",
    "clean:nodemodules": "rm -rf node_modules packages/*/node_modules",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prebuild": "npm run lint",
    "prettier": "prettier -c --config .prettierrc {.github,./*}",
    "prettier:fix": "prettier -w --config .prettierrc {.github,./*}",
    "build": "node -r ts-node/register packages/scripts/src/build.ts",
    "doc": "node -r ts-node/register packages/scripts/src/bundle-doc.ts",
    "test": "",
    "test:without-coverage": "",
    "test:watch": "",
    "test:snapshot:update": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romcal/romcal.git"
  },
